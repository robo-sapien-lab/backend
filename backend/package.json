{
  "name": "avyra-edai-backend",
  "version": "1.0.0",
  "description": "Backend API for Avyra EDAI - Educational AI Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "docker:build": "docker build -t avyra-edai-backend .",
    "docker:run": "docker run -p 8080:8080 avyra-edai-backend",
    "deploy:cloudrun": "bash deploy-cloudrun.sh",
    "deploy:cloudrun:win": "deploy-cloudrun.bat"
  },
  "keywords": [
    "education",
    "ai",
    "vertex-ai",
    "firestore",
    "supabase"
  ],
  "author": "Avyra Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/documentai": "^8.0.0",
    "@google-cloud/firestore": "^7.0.0",
    "@google-cloud/storage": "^7.0.0",
    "@google-cloud/vertexai": "^0.1.0",
    "@supabase/supabase-js": "^2.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.0",
    "express-rate-limit": "^6.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
